{
    "name": "@qazuor/qzpay-nestjs",
    "version": "1.0.1",
    "description": "NestJS integration for QZPay billing library",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": ["dist"],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "typecheck": "tsc --noEmit",
        "test": "vitest",
        "test:coverage": "vitest --coverage",
        "lint": "biome check --write src"
    },
    "dependencies": {
        "@qazuor/qzpay-core": "workspace:*"
    },
    "peerDependencies": {
        "@nestjs/common": "^10.0.0 || ^11.0.0",
        "@nestjs/core": "^10.0.0 || ^11.0.0",
        "reflect-metadata": "^0.1.13 || ^0.2.0",
        "rxjs": "^7.0.0"
    },
    "peerDependenciesMeta": {
        "@nestjs/common": {
            "optional": false
        },
        "@nestjs/core": {
            "optional": false
        }
    },
    "devDependencies": {
        "@nestjs/common": "^11.1.0",
        "@nestjs/core": "^11.1.0",
        "@nestjs/testing": "^11.1.0",
        "@qazuor/qzpay-drizzle": "workspace:*",
        "@testcontainers/postgresql": "^10.21.3",
        "drizzle-orm": "^0.39.0",
        "postgres": "^3.4.5",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "tsup": "^8.3.0",
        "typescript": "^5.7.0",
        "vitest": "^2.1.0"
    },
    "keywords": [
        "nestjs",
        "billing",
        "payments",
        "subscriptions",
        "qzpay"
    ],
    "author": "Qazuor",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/qazuor/qzpay.git",
        "directory": "packages/nestjs"
    }
}
