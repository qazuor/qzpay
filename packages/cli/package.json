{
    "name": "@qazuor/qzpay-cli",
    "version": "1.1.0",
    "description": "CLI for scaffolding QZPay billing setups",
    "type": "module",
    "bin": {
        "qzpay": "./dist/index.js"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "typecheck": "tsc --noEmit",
        "test": "vitest",
        "test:run": "vitest run",
        "clean": "rm -rf dist",
        "lint": "biome check --write src"
    },
    "keywords": [
        "qzpay",
        "billing",
        "cli",
        "scaffold",
        "stripe",
        "mercadopago"
    ],
    "author": "qazuor@gmail.com",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/qazuor/qzpay",
        "directory": "packages/cli"
    },
    "bugs": {
        "url": "https://github.com/qazuor/qzpay/issues"
    },
    "homepage": "https://github.com/qazuor/qzpay/tree/main/packages/cli#readme",
    "engines": {
        "node": ">=22"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@clack/prompts": "^0.9.1",
        "commander": "^13.1.0",
        "picocolors": "^1.1.1"
    },
    "devDependencies": {
        "@types/node": "^22.0.0",
        "tsup": "^8.3.0",
        "typescript": "^5.7.0",
        "vitest": "^2.1.0"
    }
}
